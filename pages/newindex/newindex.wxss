.map-loading {
  position: fixed;
  top: 0;
  width: 100vw;
  height: 50vh;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.map-loading-circle {
  width: 150px;
  height: 150px;
  background-color:  #7591AD;
  border: 10px solid #2c3e50;
  box-shadow: 0 0 0 5px #F97F51;
  border-radius: 50%;
}

.map-loading-wave {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #F97F51;
  border-radius: 50%;
  box-shadow: inset 0 0 50px #EC3E27;
  overflow: hidden;
  z-index: 0;
}

.map-loading-wave::before, .map-loading-wave::after {
  position: absolute;
  content: "";
  width: 200%;
  height: 200%;
  top: 0;
  left: 50%;
  transform: translate(-50%, -50%);
}

.map-loading-wave::before {
  border-radius: 30%;
  background-color: #f0932b;
  animation: animate 5s linear infinite;
}

.map-loading-wave::after {
  border-radius: 40%;
  background-color: #ffbe76;
  animation: animate 5s linear infinite;
}

@keyframes animate{
  0%{
      top:0;
  transform: translate(-50%, -50%) rotate(0deg);
  }
  100%{
      top:-100px;
  transform: translate(-50%, -50%) rotate(360deg);
  }
}



